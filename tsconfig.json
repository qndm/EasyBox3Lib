{
    "compilerOptions": {
        "allowJs": true,
        "checkJs": false,
        "noEmit": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "typeRoots": ["./types"],
        "types": ["Box3", "Box3-Pro", "Box3-Pro-client"]
    },
    "include": [
        "script/*"
    ],
    "baseUrl": "./"
}